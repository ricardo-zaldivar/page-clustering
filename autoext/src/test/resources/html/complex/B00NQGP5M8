    <script type="text/javascript">
        P.when("jQuery").execute(function ($) {
            var form = $("#addToCart"),
                thirdParty = $("#soldByThirdParty"),
                args = {
                    a: form.find("[name=ASIN]").val(),
                    me: form.find("[name=merchantID]")
                        .val(),
                    olid: form.find("[name=offerListingID]").val()
                };
            
            $('#selectQuantity [name="quantity"]').change(        
                function () {
                    args.qt = this.value;
                    
                    
                    $.get("/gp/product/du/quantity-sip-update.html",
                            args,        
                            function (data) { 
                                data = $(data);
                                thirdParty.find(".price3P").html(data.filter(".price").text());
                                
                                  thirdParty.find(".shipping3P").html(data.filter(".plusShippingText").text());
                                
                                
                                
                            }
                    );
                }
            );
        });
    </script>



		            
		    
		            
		            
		                
		                    





		                
		            
		    
		            
		            
		            
		    
		            
		            
		
		            
		            



	
		    
		            
		             





   
    
    
    
        
        <script type="text/javascript">

P.when("A","a-modal").execute(function(A, modal) {
    var offeringID = null;
    var bundleAsin = null;
    // sim kit quantity is always equal to the device quantity
    var quantity = isPositiveInteger(A.$('#quantity').val()) ? A.$('#quantity').val() : "1";
    var sessionId = A.$("#session-id").val();
    // since our input elements start from 3
    var itemCount = "4";
    var logMetrics = false;
    var installmentMetaData;
    var qualifiedBuyBoxFocused = true;

    //initialize the UI and data whenever the page reloads
    initializeModule();

    function initializeModule() {
        //enable the checkboxes only when this script loads
        A.$("input[id^=bundle_asin_]").prop("disabled", false);
        
        //clears the checkboxes whenever user comes by pressing 'back' button in the
        //browser
        A.$("input[id^=bundle_asin_]").prop('checked', false);
        removeATCFormValues();

        var marketplaceId = A.$("#simBundleMarketplaceId").val();
        // log metrics only for US marketplace
        if(marketplaceId === "ATVPDKIKX0DER") {
            logMetrics = true;
        }

        //log module view metrics
        var isSimpleBundleSectionDisplayed = A.$(".showBundleAsins").length > 0;
        if(logMetrics && isSimpleBundleSectionDisplayed) {
            logSimpleBundleMetrics("SIMBUNDLE_IMPRESSION", sessionId);
        }

        // check for the installments accordion
        installmentMetaData = A.state("installmentsMetaData");
        if (installmentMetaData) {
            // build the callback events should the accordion be toggled.
            var accordionName = installmentMetaData.accordionName;
            var installmentAccordionRowName = installmentMetaData.installmentRowName;
            var installmentAccordionId = installmentMetaData.installmentBoxId;
            var buyboxAccordionName = installmentMetaData.buyInFullRowName;
            var buyboxAccordionId = installmentMetaData.buyInFullId;

            // Set the callbacks required.
            var installmentAccordionDeclarativeAction = formatAccordionDeclarativeActionString(accordionName, installmentAccordionRowName);
            var buyboxAccordionDeclarativeAction = formatAccordionDeclarativeActionString(accordionName, buyboxAccordionName);

            // Function that acts on each checkbox input that represents each sim bundle choice.
            var inputSelectorCallback = function() {
                if (this.checked) {
                    // a checkbox was already selected before, set it to the state of the form
                    setBundleAsin(this.value);
                    setATCFormValues();

                    // update the form to reflect the appropriate quantity
                    updateQuantityandItemCountInAtcForm();

                    // break out of loop
                    return false;
                }
            };

            A.on(installmentAccordionDeclarativeAction, function(data) {
                qualifiedBuyBoxFocused = false;

                // Reset the values on the ATC Form
                removeATCFormValues();

                // Set the quantity here but not in the form.
                updateQuantity("1");

                // set the selections (if any) onto the common simple bundle form inputs
                A.$(formatInputSelector(installmentAccordionId)).each(inputSelectorCallback);
            });

            A.on(buyboxAccordionDeclarativeAction, function(data) {
                qualifiedBuyBoxFocused = true;

                // Reset the values on the ATC Form
                removeATCFormValues();

                // Set the quantity based on the current dropdown
                setQualifiedBuyboxQuantity();

                // set the selections if any onto the common simple bundle form inputs
                A.$(formatInputSelector(buyboxAccordionId)).each(inputSelectorCallback);
            });
        }
    }

    function formatInputSelector(id) {
        return "#" + id + " input[id^=bundle_asin_]";
    }

    function formatAccordionDeclarativeActionString(name, rowName) {
        return "a:accordion:" + name + ":" + rowName + ":" + "select";
    }

    function setBundleAsin(asin) {
        var $asin = A.$("#asin_" + asin);
        var $offeringId = A.$("#offeringID_" + asin);

        // in the event that $asin is of length > 1, extract the value differently
        bundleAsin = $asin.val();
        offeringID = $offeringId.val();
    }

    function logSimpleBundleMetrics(logType, sessionId) {
        var simpleBundleMetricsHandler = "/wl/srbi/metrics/log";
        var payload = buildSimpleBundleMetricsRequestPayload(logType, sessionId);
        var attempt = 0;

        var retry = function() {
            attempt++;
            A.post(simpleBundleMetricsHandler, {
                params: payload,
                success: function (json) {
                    // no-op
                },
                error: function (xhr, textStatus, errorText) {
                    if (attempt < 3) {
                        retry();
                    }
                }
            });
        };
        retry();
    }

    function buildSimpleBundleMetricsRequestPayload (logType, sessionId) {
         var params = {
            logType   : logType,
            sessionId : sessionId
        };
        return {
            payload : JSON.stringify(params)
        };
    }

    var setQualifiedBuyboxQuantity = function() {
        var dropDownQuantityValue = A.$('#quantity').val();
        updateQuantity(dropDownQuantityValue.toString());
    };

    var updateQuantity = function(qty) {
        if (isPositiveInteger(qty)) {
            quantity = qty;
        } else {
            quantity = "1";
        }
        updateQuantityandItemCountInAtcForm();
    };

    //bind Quantity dropdown to update quantity of bundle ASINs
    A.$('#quantity').change(function() {
        setQualifiedBuyboxQuantity();
        updateQuantityandItemCountInAtcForm();
    });

    var logAddToCartMetric = function() {
        if(logMetrics && (A.$("input[id^=bundle_asin_]:checked").length)) {
            logSimpleBundleMetrics("SIMBUNDLE_ADDTOCART", sessionId);
        }
    };

    // log add to cart metrics on installments button if it exists
    var $installmentAtcButton = A.$('#installments-button');
    if ($installmentAtcButton.length) {
        $installmentAtcButton.click(logAddToCartMetric);
    }

    //log add to cart metrics
    A.$('#add-to-cart-button').click(logAddToCartMetric);

    //bind click event to checkboxes
    A.$("input[id^=bundle_asin_]").click(function(e) {     
        // if click is triggered by code, we need to set 'checked' property
        // explicitly
        if ((!e.which) && (!this.checked)) {
            this.checked = true;
        }

        // only modify the checkboxes within the container
        var $selected = A.$(this);
        var $parentContainer = $selected.closest(".showBundleAsins");
        // give radio button functionality to checkboxes
        $parentContainer.find("input[id^=bundle_asin_]").not(this).prop('checked', false);

        //bounce input fields of bundle asins to add-to-cart form
        removeATCFormValues();

        if ($parentContainer.find("input[id^=bundle_asin_]:checked").length === 1) {
            var checkedBundleAsin = this.value;
            bundleAsin = A.$("#asin_" + checkedBundleAsin).val();
            offeringID = A.$("#offeringID_" + checkedBundleAsin).val();
            setATCFormValues();
        }
    });

    // input elements start from 3 as first 2 are reserved for device protection
    // options
    function setATCFormValues() {
        //enabling the input elements of ATC form        
        A.$("#useABBHandler").prop('disabled', false);
        A.$("#quantity\\.3").prop('disabled', false);
        A.$("#asin\\.3").prop('disabled', false);
        A.$("#offeringID\\.3").prop('disabled', false);
        // If device protection feature is present disable our itemCount"
        // 2 itemCount params in POST call will result in a 500.
        if (A.$("#abbWrapper [name='itemCount']").length === 0) {
            A.$(".simple-bundle-inputs #itemCount").prop('disabled', false);
        }

        //set values of input elements
        A.$("#quantity\\.3").val(quantity);
        A.$("#asin\\.3").val(bundleAsin);
        A.$("#offeringID\\.3").val(offeringID);
        A.$(".simple-bundle-inputs #itemCount").val(itemCount);
    }

    function removeATCFormValues() {
        //disabling the input elements of ATC form
        A.$("#useABBHandler").prop('disabled', true);
        A.$("#quantity\\.3").prop('disabled', true);
        A.$("#asin\\.3").prop('disabled', true);
        A.$("#offeringID\\.3").prop('disabled', true);
        A.$(".simple-bundle-inputs #itemCount").prop('disabled', true);

        //default values of input elements
        A.$("#quantity\\.3").val("0");
        A.$("#asin\\.3").val("");
        A.$("#offeringID\\.3").val("");
        A.$(".simple-bundle-inputs #itemCount").val("1");
    }

    function updateQuantityandItemCountInAtcForm() {
        var quantityObj = A.$('#quantity\\.3');
        if(quantityObj.length) {
            quantityObj.val(quantity);
        }
    }

    // Hide Sim Starter Text if no bundle asin is present
    if(A.$('input[id^="bundle_asin_"]').length === 0) {
        A.$('#add_sim_starter_text').hide();
    }

    // Show review tab inside the popover on click
    A.declarative('show-review-tab', 'click', function(event) {
        var reviewTab = A.$(".popoverReviewsTab");
        reviewTab.click();
    });

    // Add to Order button handler in secondaryView
    A.declarative('add-to-cart', 'click', function(event) {
        var data = event.data;
        var selector = "[id^=bundle_asin_" + data.asin + "]";
        // need to determine if popover should add to installment or common buy box's checkboxes
        if (installmentMetaData) {
            if (qualifiedBuyBoxFocused) {
                // selects the input only within the appropriate accordion
                selector = "#" + installmentMetaData.buyInFullId + " " + selector;
            } else {
                selector = "#" + installmentMetaData.installmentBoxId + " " + selector;
            }
        }
        var targetCheckbox = A.$(selector);
        if (!targetCheckbox.prop('checked')) {
            // triggers the click function handler
            targetCheckbox.click();
            targetCheckbox.prop('checked', true);
        }
        hidePopover(data.asin);
    });

    // No Thanks button handler in secondaryView
    function hidePopover(asin) {
        A.$("[id^=popover-simpleBundle-]").each(function() {
            var modalInstance = modal.get(A.$(this));
            if (modalInstance.isActive()) {
                modalInstance.hide();
            }
        });
    }

    // Change Image on mouse hover
    A.declarative('focus-image', 'mouseenter', function(event) {
        var data = event.data;
        // Change the image
        A.$("#productImageMain-" + data.asin).prop('src', data.imageUrl);

        // Remove selected style from all other thumbnails
        A.$(".thumbnail-group").toggleClass("a-button-selected", false);

        // Highlight the current thumbnail
        A.$("#" + event.$target.prop("name")).toggleClass("a-button-selected", true);
    });

    function isPositiveInteger(stringVal) {
        return /^\+?([1-9]\d*)$/.test(stringVal);
    }

});

</script>

        <style type="text/css">
.popover-tab > .a-box-inner {
        height: 400px;
        overflow-y: auto;
}
</style>


