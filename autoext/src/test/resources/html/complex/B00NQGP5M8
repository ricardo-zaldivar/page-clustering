    P.when("jQuery").execute(function(jQuery) { 
        P.when("reviewsLightbox-js").execute(function() {
            jQuery("#DP_IMAGE_GALLERY_BUTTON_CONTAINER").find(".gallery-link").bind("click", function() {
                jQuery("#DP_IMAGE_GALLERY_BUTTON_CONTAINER").find(".a-popover-trigger").click();  
                P.when("reviewsLightbox-ready-DP_IMAGE_GALLERY_BUTTON").execute(function() {
                    jQuery("#DP_IMAGE_GALLERY_BUTTON").trigger("hideImmersiveView");
                });
            });
            
            jQuery("#DP_IMAGE_GALLERY_BUTTON_CONTAINER").find(".image-thumbnails").each(function(index) {
                jQuery(this).bind("click", function() {
                    jQuery("#DP_IMAGE_GALLERY_BUTTON_CONTAINER").find(".a-popover-trigger").click();  
                    P.when("reviewsLightbox-ready-DP_IMAGE_GALLERY_BUTTON").execute(function() {
                        jQuery("#DP_IMAGE_GALLERY_BUTTON").trigger("jumpToImageAtIndex", index);
                    });
                });
            });
